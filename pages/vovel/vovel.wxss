/* pages/vovel/vovel.wxss */
page{
  background: #fff;
}
.container {
  position: relative;
  width: 100%;
  height: 100%;
  background-color: #fff;
  color: #939393;
  padding: 0rpx;
}

.customSwiper {
  height: 420rpx;
  width: 100%;
  padding: 0rpx;
  margin-top: 0rpx;
  position: relative;
  }
.banner{height: 400px;background-color:#ddd;}
.customSwiper swiper{height: 100%;}
.tab{
  height: 7rpx;position: absolute;bottom: 10rpx;display:flex;
  width: 100%;text-align: center;justify-content:center;
  align-items: center;
}
.tab .tab-item{ background-color: #ccc;height: 7rpx;width: 50rpx;
line-height: 50rpx;font-size:12rpx;color:#fff;border-radius: 4px;
margin-right:10px;}
.tab .tab-item.selected{ background-color: #20b7f5;}

.menuList{
  height: 180rpx;
  width: 100%;
  padding: 0rpx;
  margin-top: 0rpx;
  position: relative;
}
.menu_item{
  float: left;
  width: 50%;
  height: 120px;
  text-align: center;
}
.menu_item image{
  width: 100rpx;
  height: 100rpx;
  margin-top: 24rpx;
}
.menu_item text{
  font-size: 15px;
  color: #323232;
}

.cate{
  top: 0;
  right: 0;
  flex: 1;
  width: 100%;
  min-height: 100vh;
  padding: 0px;
  background: #fff;
  box-sizing: border-box;
}
.sub_cate{
  width: 100%;
  float: left;
  padding: 0px;
}
.sub_cate_title
{
  margin-left: 0px;
  margin-bottom: 0px;
  height: 50px;
  display: flex;
  align-items: center;
}
.sub_cate_title text
{
  margin-left: 30px;
  font-size: 16px;
  color: #323232;
}
.sub_cate_item{
  float: left;
  width: 33.33%;
  height: 120px;
  text-align: center;
}
.sub_cate_item image{
  width: 64.62px;
  height: 90px;
  margin-top: 4px;
}
.sub_cate_item text{
  display: block;
  margin-top: 0px;
  font-size: 15px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  color: #323232;
}

.line_view{
  background-color: #E5E5E5;
  height: 1.2px;
  margin-left: 20px;
  margin-bottom: 0px;
}